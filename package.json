  "keywords": [
    "sharepoint",
    "webhooks",
    "microsoft-graph",
    "azure-functions",
    "proxy"
  ],
  "author": "FAM Brands LLC",
  "license": "Proprietary",
  "dependencies": {
    "@azure/data-tables": "^13.2.2",
    "@azure/functions": "^4.7.2-preview",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5"
  }
}
