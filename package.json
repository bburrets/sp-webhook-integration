{
  "name": "sharepoint-webhooks",
  "version": "2.0.0",
  "description": "Production-ready SharePoint webhook management and proxy solution for Azure Functions",
  "main": "src/functions/*.js",
  "scripts": {
    "start": "func start",
    "deploy": "func azure functionapp publish webhook-functions-sharepoint-002",
    "logs": "func azure functionapp logstream webhook-functions-sharepoint-002",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\" \"scripts/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"test/**/*.js\" \"scripts/**/*.js\"",
    "quality": "npm run lint && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "clean": "rm -rf node_modules package-lock.json"
  },
  "keywords": [
    "sharepoint",
    "webhooks",
    "microsoft-graph",
    "azure-functions",
    "proxy"
  ],
  "author": "FAM Brands LLC",
  "license": "Proprietary",
  "dependencies": {
    "@azure/data-tables": "^13.2.2",
    "@azure/functions": "^4.7.2-preview",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "eslint": "^8.57.0",
    "jest": "^30.0.5",
    "prettier": "^3.1.0"
  }
}
